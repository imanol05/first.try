list_enteros = []
while True:
  num_enteros = int(input("Ingrese un numero entero entre el 0 y el 9\n-------->"))
  while num_enteros > 9 or num_enteros < 0:
    num_enteros = int(input("El numero entero ingresado es incorrecto, por favor ingrese un numero entre el 1 y el 9\n-------->"))
  if num_enteros in list_enteros:
    print("El numero ya se encuentra en la lista de enteros, bye")
    break
  list_enteros.append(num_enteros)
  print(list_enteros)